class Cat {
    String name;
    String color;

    Cat(String name, String color) {
        this.name = name;
        this.color = color;
    }

    void meow() {
        System.out.println(name + " is meowing.");
    }
}

public class Main {
    public static void main(String[] args) {
        Cat myCat = new Cat("Whiskers", "black");
        myCat.meow();
    }
}
