public class Main {
    public static void main(String[] args) {
        int n = 3;  // Example value for n
        int arr[] = new int[100];
        int j = 1;
        
        for (int i = 1; i < 100; i++) {
            if (i % 2 != 0) {
                arr[j] = i;
                j++;
            }
        }
        
        System.out.print(arr[n * 2]);
    }
}
